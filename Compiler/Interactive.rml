	  => Absyn.Comment option =

axiom	set_class_comment_in_comment_opt(SOME(Absyn.COMMENT(ann,_)),"") 
	  => SOME(Absyn.COMMENT(ann,NONE))

  axiom	set_class_comment_in_comment_opt(SOME(Absyn.COMMENT(ann,_)),cmt) 
