   axiom	delete_equation_in_eqlist([],_,_) => []

   rule	Absyn.cref_to_path(c1) => p1 & Absyn.path_string(p1) => s1 &
	 Absyn.cref_to_path(c2) => p2 & Absyn.path_string(p2) => s2 &
