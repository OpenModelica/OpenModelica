relation lbinop_symbol : Exp.Operator => string =

  axiom	lbinop_symbol(Exp.AND) => " && "

  axiom	lbinop_symbol(Exp.OR) => " || "

end

(** relation: lunaryop_symbol
 **
 ** Helper relation to print_exp2_str
 **)
relation lunaryop_symbol : Exp.Operator => string =
