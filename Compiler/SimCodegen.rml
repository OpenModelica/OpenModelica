	int_string(unique_id) => id_str &
	Util.string_append_list([str," = get_vector_elt(b",id_str,",",index_str,");"])
	=> stmt &
	  Codegen.c_add_statements(cfunc,[stmt]) => cfunc' 
	----------------------
	generate_ode_system2_collect_results2(str::strs,index,unique_id) 
	  => cfunc'    
end

