        txt = Tpl.nextIter(txt);
        (txt, a_preExp) = lm_135(txt, rest, a_simCode, a_preExp, a_context);
      then (txt, a_preExp);

    case ( txt,
           _ :: rest,
           a_simCode,
           a_preExp,
           a_context )
      equation
        (txt, a_preExp) = lm_135(txt, rest, a_simCode, a_preExp, a_context);
      then (txt, a_preExp);
  end matchcontinue;
end lm_135;

