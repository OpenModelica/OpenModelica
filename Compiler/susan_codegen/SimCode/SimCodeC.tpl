  <<
  mixed_equation_system(<numDiscVarsStr>);
  double values[<valuesLenStr>] = {<values of val: '<val>' ", ">};
  int value_dims[<numDiscVarsStr>] = {<value_dims of dim: '<dim>' ", ">};
