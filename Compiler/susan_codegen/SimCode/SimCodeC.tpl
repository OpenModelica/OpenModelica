underscorePath(Path path) ::=
  match path
  case QUALIFIED then
    '<System.stringReplace(name, "_", "__")>_<underscorePath(path)>'
  case IDENT then
    System.stringReplace(name, "_", "__")
  case FULLYQUALIFIED then
    underscorePath(path)

