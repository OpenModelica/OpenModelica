algorithm
  out_txt :=
  matchcontinue(in_txt, in_i_it, in_i_context)
    local
      Tpl.Text txt;
      SimCode.Context i_context;

    case ( txt,
           DAE.STMT_ASSIGN(exp1 = DAE.CREF(componentRef = DAE.WILD()), exp = i_e),
           i_context )
