  "der(elastoBacklash.w_rel)",
  "der(inertia2.w)",
  "der(inertia1.w)",
  "inertia1.flange_a.tau",
  "inertia1.phi",
  "inertia2.flange_a.tau",
  "inertia2.phi",
  "inertia1.flange_b.tau",
  "inertia2.flange_b.tau"});

