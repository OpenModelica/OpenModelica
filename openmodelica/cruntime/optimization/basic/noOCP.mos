setCommandLineOptions("+gDynOpt");

loadString(
"
model foo
  parameter Real p = 1;
  Real y = time;
end foo;
"
);
getErrorString();

optimize(foo, numberOfIntervals=5, tolerance = 1e-8);
getErrorString();

