// name: cseTestCall6
// keywords: cse
// status: correct

loadString("
package CSE
  model TestCall6
    record rec
      Real x;
      Real y;
    end rec;
    
    Real a;  
    
    function f
      input Real s;
      output rec t;
    algorithm 
      t.x := s;
      t.y := s+1;
    end f;
  
    function f2
      input rec s;
      output Real t;
    algorithm 
      t := s.x+s.y;
    end f2;
  
  equation 
    a = f2(f(time));
  end TestCall6;
end CSE;
"); getErrorString();

setDebugFlags("dumpCSE"); getErrorString();
setCommandLineOptions("--postOptModules+=wrapFunctionCalls"); getErrorString();
simulate(CSE.TestCall6); getErrorString();

