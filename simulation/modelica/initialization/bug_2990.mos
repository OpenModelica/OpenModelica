// name: bug_2990
// keywords: initialization
// status: correct
// cflags:
// teardown_command: rm -rf initializationTests.bug_2990* _initializationTests.bug_2990* output.log bug_2990.mat

loadString("
within ;
package initializationTests
  model bug_2990
    parameter Real L = 2.5;
    parameter Real a = 0.24;
    Real x(start = 1.0, min=-L^2.0, max=L^2.0);
    Real y;
  equation
    der(x)=y;
    der(y)=-a*x;
  end bug_2990;
end initializationTests;
"); getErrorString();

simulate(initializationTests.bug_2990, simflags="-lv=LOG_INIT"); getErrorString();
simulate(initializationTests.bug_2990, simflags="-lv=LOG_INIT -iim=none"); getErrorString();

