loadString("
package Test
  model M
    parameter Real p(fixed = false);
  initial equation
    20 = p*10 + 10;
  end M;

  model Q
    M m;
  end Q;
end Test;
"); getErrorString();

simulate(Test.Q, fileNamePrefix="Test.Q_xxx"); getErrorString();
val(m.p, {0.0, 0.5});

simulate(Test.Q, simflags="-iim=none -iif=Test.Q_xxx_res.mat -iit=1.0 -lv=LOG_INIT"); getErrorString();
val(m.p, {0.0, 0.5});

