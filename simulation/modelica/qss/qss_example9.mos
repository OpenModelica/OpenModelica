// name: qss_example9
// status: correct

loadString("
within ;
package qssTests
  model example9
    Real x(start=1.0, fixed=true);
  equation
    der(x) = sin(time);
  end example9;
end qssTests;
"); getErrorString();

setCommandLineOptions("--postOptModules+=addTimeAsState"); getErrorString();
simulate(qssTests.example9, method="qss"); getErrorString();
val(x, {0.0, 1.0}); getErrorString();

