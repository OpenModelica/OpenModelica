// name: TestSolve
// status: correct
//
// Checks that the backend knows how to solve certain expressions
//

loadString("
model ModelTestSolve6
  Real x(start=0);
  Real y(start=4);
  Real z(start=10);
  Real w;
equation
  cos(x) = time;
  cos(y) = time;
  cos(z) = time;
  w = acos(time);
end ModelTestSolve6;
"); getErrorString();

simulate(ModelTestSolve6, simflags="-lv LOG_NLS_V"); getErrorString();
val(x,0.5);
val(y,0.5);
val(z,0.5);
val(w,0.5);

