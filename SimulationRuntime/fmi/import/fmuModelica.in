  function Bool2Int
    input Integer dim;
    input Boolean in_bv[dim];
    output Integer out_iv[dim];
  algorithm
    for i in 1:dim loop
          out_iv[i]:=if in_bv[i] then 1 else 0;
    end for;
  end Bool2Int;

  function Int2Bool
    input Integer dim;
    input Integer in_iv[dim];
    output Boolean out_bv[dim];
  algorithm
    for i in 1:dim loop
          out_bv[i]:=if in_iv[i] == 1 then true else false;
    end for;
  end Int2Bool;

