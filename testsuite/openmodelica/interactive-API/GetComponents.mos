// name: GetComponents
// keywords: 
// status: correct
// cflags: -d=-newInst
//
// Tests the getComponents API functions.
//

loadString("
  model A
    Real x;
    Real y;
  end A;

  model B
    extends Foo;
    Real a;
    Real b;
  end B;

  model C
    A a;
    B b;
  end C;

  model D
    parameter Real r1 = 0.1 annotation(Dialog(group = \"Group\", tab = \"Tab\")), r2;
  end D;

  model E
    Foo f;
    A a;
  end E;

  model F
    model G
      type T = Real;

      model F
        model H
          F.G.T x;
        end H;
      end F;
    end G;
  end F;
"); getErrorString();

getComponents(A);
getComponents(B);
getComponents(C);
getComponents(D);
getComponents(E);
getComponents(F.G.F.H);

