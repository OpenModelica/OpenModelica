// name: ListExpressions
// keywords: list expression #2607
// status: correct
// cflags: -d=-newInst
// 
// Tests dumping of various expression with list, to make sure that parentheses
// are correctly used.
//

loadString("
  model M
  equation
    0 = (a - b) - (c - d);
    0 = (a + b) + (c + d);
    0 = a - b - c - d;
    0 = a + b + c + d;
    0 = (a - b) + (c - d);
    0 = a / (b / c);
    0 = a / b / c;
    0 = a * (b * c);
    0 = a * b * c;
    0 = a * ((a * b)/(c*d));
  end M;
");
getErrorString();

list(M); getErrorString();

