loadString("
  package P
    model A
      Real x;
      Real y;
    protected
      Real u;
      Real w;

      class C
        Real x;
      end C;

      C c;
    equation
      x = y;
    algorithm
      x := y;
    end A;
  end P;
");

loadClassContentString("
    Real z;
    Real x;

    class C
      Real y;
    end C;

    C c;
  equation
    x = z;
  algorithm
    y := z;
  protected
    Real p;
", P.A);

list(P.A);

