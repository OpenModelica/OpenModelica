loadString("
  package P1
    model M
      model M1
      end M1;
    end M;

    model A
      extends P1.M;
    end A;

    model B
      extends M;
    end B;
  end P1;

  package P2
    model A
      extends P1.M;
    end A;

    model B
      replaceable class M1 = P1.M;
      replaceable class M2 = .P1.M;
    end B;

    package P1
      model M end M;

      model A
        P1.M m;
      end A;
    end P1;
  end P2;

  package P3
    model A
      P1.M m;
      P1.M.M1 m1;
    end A;
  end P3;
");

reverseLookup(P1.M, P3, exactMatch = true, prettyPrint = true);
getErrorString();

