  {"inertia3.phi",
  "inertia3.w",
  "clutch.phi_rel",
  "clutch.w_rel",
  "spring.phi_rel",
  "spring.w_rel",
  "der(inertia3.phi)",
  "der(inertia3.w)",
  "der(clutch.phi_rel)",
  "der(clutch.w_rel)",
  "der(spring.phi_rel)",
  "der(spring.w_rel)"});

