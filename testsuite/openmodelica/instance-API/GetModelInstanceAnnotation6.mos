loadString("
  model M
    block To_deg
      Real y;
    end To_deg;

    model RotatingRect
      To_deg to_deg;
    equation
      annotation(Icon(graphics = {Rectangle(extent = {{0, 0}, {1, 1}}, rotation = DynamicSelect(-5, to_deg.y))}));
    end RotatingRect;

    RotatingRect rotatingRect;
  end M;
");

getModelInstance(M, prettyPrint=true);

