loadString("
  model A
  end A;

  model B
    parameter Boolean b = false;
    replaceable model C = A constrainedby A annotation (Dialog(enable=b));
    C c;
  end B;

  model M
    extends B;
  end M;
");

getModelInstance(M, prettyPrint = true);
getErrorString();

