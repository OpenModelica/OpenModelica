loadString("
  connector C
    Real e;
    flow Real f;
    stream Real s;
  end C;

  model A
    replaceable Real w;
    outer Real z;
  end A;

  model M
    final Real x;
    input Real y;
    inner Real z;
    extends A;
  protected
    redeclare replaceable Real w;
    parameter C c;
  end M;
");

getModelInstance(M, prettyPrint = true);

