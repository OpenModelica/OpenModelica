loadString("
  model A
    Real x;
  end A;

  model M
    A a;
    Real y = a.x;
    Real z = y;
  end M;

  model N
    extends M;
  end N;
");

getModelInstance(N, context = b.c, prettyPrint = true);
getErrorString();

