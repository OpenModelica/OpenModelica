loadString("
  model HierarchicalEditingGUI
    model Nested1
      parameter Real p1A;
      parameter Real p1B;
      replaceable model InnerNestedModel = Nested2 annotation(choicesAllMatching = true);
      InnerNestedModel nested2;
    equation
    end Nested1;

    model Nested2
      parameter Real p2A;
    equation
    end Nested2;

    model Main
      parameter Real passthrough_p1A;
      replaceable model NestModel = Nested1 annotation(choicesAllMatching = true);
      NestModel nested1(p1A=passthrough_p1A) annotation(
        Placement(transformation(extent = {{-10, -10}, {10, 10}})));
    equation
    end Main;

    model Problem1
    Main main(
          passthrough_p1A = 11,
          nested1(p1B=2, nested2(p2A=3))
        )  annotation(
        Placement(transformation(extent = {{-10, -10}, {10, 10}})));
    equation
    end Problem1;
  equation
  end HierarchicalEditingGUI;
");

getModelInstance(HierarchicalEditingGUI.Problem1, prettyPrint = true); getErrorString();

