loadString("
  model A
    Real x;
    Real y;
  end A;

  model M
    A a1[2](final x = {2.0, 3.0}, each y = 1);
    A a2[3](x = {1.0, 2.0, 3.0}, each final y = 2);
  end M;
");

getModelInstance(M, prettyPrint = true);

