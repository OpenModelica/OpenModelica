loadString("
  package P
    model World
      parameter Configuration conf1;
      parameter Configuration conf2 = conf1;
    end World;

    record Configuration
    end Configuration;

    model A
      outer World world;
    end A;

    model M
      inner World world;
      A a;
    end M;
  end P;
");

getModelInstance(P.M, prettyPrint = true);

