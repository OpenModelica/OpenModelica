loadString("
  model M
    parameter Boolean b = true
     annotation(
       choices(
         choice = 1.0 \"A\",
         choice = 2.0 \"B\",
         choice = 3.0 \"C\",
         checkBox = true));
  end M;
");

getModelInstance(M, prettyPrint=true);
getErrorString();

