loadString("
  package P
    package P2
      type OtherReal = Real;
    end P2;

    model M
      replaceable Real x annotation(
        choices(
          choice(redeclare Real x = 1.0 \"A\"),
          choice(redeclare Real x = 2.0 \"B\"),
          choice(redeclare P2.OtherReal x = 3.0 \"C\"),
          choice(redeclare MissingReal x = 4.0 \"D\")));
    end M;
  end P;
");

getModelInstance(P.M, prettyPrint=true);
getErrorString();

