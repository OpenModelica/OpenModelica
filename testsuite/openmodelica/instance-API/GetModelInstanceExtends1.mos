loadString("
  model A
    Real x;
  end A;

  model M
    Real y;
    extends A(x = 1) annotation(IconMap(extent = {{-100, -100}, {100, 100}}));
    Real z;
  end M;
");
getErrorString();

getModelInstance(M, prettyPrint=true);

