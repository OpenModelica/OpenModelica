// name: ocpOnlyInputs
// status: correct
// cflags: -d=-newInst

setCommandLineOptions("+gDynOpt");

loadString(
"
model foo
  parameter Real p = 1;
  input Real u;
  Real cost = (u-sin(time))^2 annotation(isLagrange = true);
end foo;
"
);
getErrorString();

optimize(foo, numberOfIntervals=5, tolerance = 1e-8);
getErrorString();
val(cost,0.0);
val(cost,0.1);
val(cost,0.2);
val(cost,0.3);
val(cost,0.4);
val(cost,0.5);
val(cost,0.65);
val(cost,0.80);
val(cost,1.0);

