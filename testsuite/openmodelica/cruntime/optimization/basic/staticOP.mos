// name: static OP
// status: correct

setCommandLineOptions("+g=Optimica");

loadString(
"
optimization foo(objective = cost_u + cost_w)
  Real cost_u = (3-u)^2;
  Real cost_w = (7-w)^2;
  input Real u;
  input Real w;
  Real x \"dummy state\";
equation
  der(x) = 0;
end foo;
"
);
getErrorString();

optimize(foo, numberOfIntervals=1, tolerance = 1e-8, simflags ="-optimizerNP 1");
getErrorString();

val(u, 0.0);
val(u, 1.0);
val(w, 0.0);
val(w, 1.0);

