
// name:     testDumpSaveInitialGuess_system.mos
// keywords: kinsol initial guess file
// status: correct
// teardown_command: rm -rf SaveInitialGuess* output.log
// cflags: -d=-newInst
//
// Test flag -saveInitialGuess_system=/path/to/file.mat,nls_index,
// which writes the current state of the initialization to a .mat file
// and can be read in later via -iif and -stopAtSysten=nls_index
//
loadString("
model SaveInitialGuess
  Real x(start = 1);
  Real y;
equation
  x + y = 0.5;
  x^2 + y^2 = 1.0;
end SaveInitialGuess;
"); getErrorString();

simulate(SaveInitialGuess,
         simflags="-nls=experimental-kinsol -saveInitialGuess_system=savedInitialGuess.mat,4",
         startTime=0,
         stopTime=0); getErrorString();

