PHONY: TestFMU_FMU
TestFMU_FMU: nozip
	cd .. && rm -f ../TestFMU.fmu && zip -r ../TestFMU.fmu *
nozip: TestFMU_functions.h TestFMU_literals.h $(OFILES) $(RUNTIMEFILES) $(FMISUNDIALSFILES)
	$(CXX) -shared -I. -o TestFMU$(DLLEXT) $(RUNTIMEFILES) $(FMISUNDIALSFILES) $(OFILES) $(CPPFLAGS)    $(CFLAGS) $(LDFLAGS) -llis -Wl,--kill-at
	mkdir.exe -p ../binaries/win64
	dlltool -d TestFMU.def --dllname TestFMU$(DLLEXT) --output-lib TestFMU.lib --kill-at
	cp TestFMU$(DLLEXT) TestFMU.lib TestFMU_FMU.libs ../binaries/win64/
	rm -f *.o TestFMU$(DLLEXT) $(OFILES) $(RUNTIMEFILES) $(FMISUNDIALSFILES)
	cd .. && rm -f ../TestFMU.fmu && zip -r ../TestFMU.fmu *

