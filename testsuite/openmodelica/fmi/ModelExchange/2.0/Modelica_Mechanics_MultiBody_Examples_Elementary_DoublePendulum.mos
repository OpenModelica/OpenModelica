translateModelFMU(Modelica.Mechanics.MultiBody.Examples.Elementary.DoublePendulum, version="2.0");
getErrorString();
importFMU("Modelica_Mechanics_MultiBody_Examples_Elementary_DoublePendulum.fmu");
getErrorString();
loadFile("Modelica_Mechanics_MultiBody_Examples_Elementary_DoublePendulum_me_FMU.mo");
getErrorString();
simulate(Modelica_Mechanics_MultiBody_Examples_Elementary_DoublePendulum_me_FMU,stopTime=3.0);
getErrorString();

val(revolute1_phi, 0);
val(revolute1_phi, 1);
val(revolute1_phi, 2);
val(revolute1_phi, 3);

val(revolute2_phi, 0);
val(revolute2_phi, 1);
val(revolute2_phi, 2);
val(revolute2_phi, 3);

