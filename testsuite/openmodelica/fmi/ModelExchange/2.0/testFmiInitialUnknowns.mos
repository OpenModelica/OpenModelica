// name:  testFMIInitialUnknowns.mos
// keywords: FMI 2.0 export
// status: correct
// teardown_command: rm -rf binaries sources modelDescription.xml modelDescription.tmp.xml initialUnknownstest.xml

loadString("
model InitialUnknown01
  input Real u;
  Real x;
  output Real y;
initial equation
  x = 0.0;
equation
  der(x) = u;
  y = x;
end InitialUnknown01;

model InitialUnknown02
  input Real u;
  Real x;
  output Real y;

  Real a, b;
initial equation
  x = 0.0;
equation
  a = 2.0*u;
  b = 0.5*a;
  der(x) = b;
  y = x;
end InitialUnknown02;

model Pendulum
  parameter Real m=0.5;
  parameter Real g=9.82;
  parameter Real L=1;
  Real x(start=L),y(start=0),xd,yd;
  Real Fo;
equation
  der(y)=yd;
  der(x)=xd;
  m*der(xd) = -x*Fo/L;
  m*der(yd) = -m*g-Fo*y/L;
  x*x+y*y=L^2;
end Pendulum;
");
getErrorString();

//buildModel(InitialUnknown01);getErrorString();

translateModelFMU(InitialUnknown01);
getErrorString();

// unzip to console, quiet, extra quiet
system("unzip -cqq InitialUnknown01.fmu modelDescription.xml > modelDescription.tmp.xml"); getErrorString();
system("sed -n \"/<ModelVariables>/,/<\\/ModelVariables>/p\" modelDescription.tmp.xml > initialUnknownstest.xml"); getErrorString();
readFile("initialUnknownstest.xml"); getErrorString();

system("sed -n \"/<ModelStructure>/,/<\\/ModelStructure>/p\" modelDescription.tmp.xml > initialUnknownstest.xml"); getErrorString();
readFile("initialUnknownstest.xml"); getErrorString();


translateModelFMU(InitialUnknown02);
getErrorString();

// unzip to console, quiet, extra quiet
system("unzip -cqq InitialUnknown02.fmu modelDescription.xml > modelDescription.tmp.xml"); getErrorString();
system("sed -n \"/<ModelVariables>/,/<\\/ModelVariables>/p\" modelDescription.tmp.xml > initialUnknownstest.xml"); getErrorString();
readFile("initialUnknownstest.xml"); getErrorString();

system("sed -n \"/<ModelStructure>/,/<\\/ModelStructure>/p\" modelDescription.tmp.xml > initialUnknownstest.xml"); getErrorString();
readFile("initialUnknownstest.xml"); getErrorString();

translateModelFMU(Pendulum);
getErrorString();

// unzip to console, quiet, extra quiet
system("unzip -cqq Pendulum.fmu modelDescription.xml > modelDescription.tmp.xml"); getErrorString();
system("sed -n \"/<ModelVariables>/,/<\\/ModelVariables>/p\" modelDescription.tmp.xml > initialUnknownstest.xml"); getErrorString();
readFile("initialUnknownstest.xml"); getErrorString();

system("sed -n \"/<ModelStructure>/,/<\\/ModelStructure>/p\" modelDescription.tmp.xml > initialUnknownstest.xml"); getErrorString();
readFile("initialUnknownstest.xml"); getErrorString();

