// name: fmi_attributes_14.mos
// keywords: FMI 2.0 export
// status: correct
// teardown_command: rm -rf fmi_attributes_14.fmu fmi_attributes_14.log fmi_attributes_14.xml fmi_attributes_14_tmp.xml fmi_attributes_14.fmutmp/

loadString("
model fmi_attributes_14
  input Real u;
  Real x;
  output Real y;
  Real a, b;
  parameter Real z=30;
protected
  Real m, n;
initial equation
  x = 0.0;
equation
  a = 2.0*u;
  b = 0.5*a;
  der(x) = b+z;
  y = x;
  m = z;
  n = a;
end fmi_attributes_14;
"); getErrorString();

setCommandLineOptions("--fmiFilter=internal"); getErrorString();
translateModelFMU(fmi_attributes_14); getErrorString();

// unzip to console, quiet, extra quiet
system("unzip -cqq fmi_attributes_14.fmu modelDescription.xml > fmi_attributes_14_tmp.xml"); getErrorString();
system("sed -n \"/<ModelVariables>/,/<\\/ModelVariables>/p\" fmi_attributes_14_tmp.xml > fmi_attributes_14.xml"); getErrorString();
readFile("fmi_attributes_14.xml"); getErrorString();

system("sed -n \"/<ModelStructure>/,/<\\/ModelStructure>/p\" fmi_attributes_14_tmp.xml > fmi_attributes_14.xml"); getErrorString();
readFile("fmi_attributes_14.xml"); getErrorString();


