model test_Bug3034
  input Real u;
  Real x;
equation
  der(x) = x+u;
end test_Bug3034;
");
getErrorString();

loadModel(Modelica);
translateModelFMU(test_Bug3034, version="2.0");
getErrorString();
importFMU("test_Bug3034.fmu");
loadFile("test_Bug3034_me_FMU.mo");
getErrorString();
simulate(test_Bug3034_me_FMU);
getErrorString();



