loadString("
model ZeroStates20
  Real x(start=1), y;
  parameter Real a=2;
equation
  1 = a * x+time;
  y = x+a;
end ZeroStates20;
");
getErrorString();
translateModelFMU(ZeroStates20, version="2.0");
getErrorString();
importFMU("ZeroStates20.fmu");
loadFile("ZeroStates20_me_FMU.mo");
getErrorString();
simulate(ZeroStates20_me_FMU);
val(y,0);
val(y,1);

