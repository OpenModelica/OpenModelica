importFMU("BouncingBall.fmu"); getErrorString();
loadFile("BouncingBall_me_FMU.mo"); getErrorString();
simulate(BouncingBall_me_FMU, stopTime=3.0); getErrorString();
val(h,0);
val(h,1);
val(h,3);

