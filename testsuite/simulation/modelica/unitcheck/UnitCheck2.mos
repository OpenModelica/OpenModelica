loadString("
package unitCheckTests
  model UnitCheck2
    Real S(unit=\"m\");
    Real V(unit=\"m/s\");
    Real A(unit=\"m/s2\");
  equation
    V = der(S);
    der(V) = A;
    S = 5.0;
  end UnitCheck2;
end unitCheckTests;
"); getErrorString();

setCommandLineOptions("--unitChecking -d=dumpUnits,newInst"); getErrorString();
instantiateModel(unitCheckTests.UnitCheck2); getErrorString();

