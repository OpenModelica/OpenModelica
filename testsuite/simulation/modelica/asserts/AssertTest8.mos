loadString("
class AssertTest
  Real x(min = 0, start=0.5);
  Real y(start = 0, fixed = true);
equation
  0 = 1e-6*x + sin(x+0.5);
  der(y) = x;
end AssertTest;
");
getErrorString();

//setDebugFlags("-warnMinMax");
getErrorString();
simulate(AssertTest);
getErrorString();

