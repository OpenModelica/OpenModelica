loadString("
model testAssertSolve
 Real x(start = 5);
 Real y(start = 1);
equation
 y = exp(x); // x = log(y) = log(time^(1/3)) 
 y^3  = time; // y = time^(1/3)
end testAssertSolve;
");
getErrorString();

simulate(testAssertSolve, stopTime=1.0);
getErrorString();
val(x,0.0); 
val(y,0.0); 
val(y,1.0);


