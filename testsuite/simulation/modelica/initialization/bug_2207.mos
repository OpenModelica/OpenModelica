loadString("
within ;
package initializationTests
  model bug_2207
    parameter Real a=100;
    constant Real b=100;
    Real x(start=2*a, nominal=3*a, min=-4*a, max=5*a);
    Real y(start=2*b, nominal=3*b, min=-4*b, max=5*b);
  equation
    der(x) = 0;
    der(y) = 0;
  end bug_2207;
end initializationTests;
"); getErrorString();

simulate(initializationTests.bug_2207, simflags="-lv=LOG_INIT"); getErrorString();


