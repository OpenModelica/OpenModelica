// cflags: -d=-newInst

loadString("
within ;
package initializationTests
  model bug_3014
    Real a \"= -1\";
    Real b \"= -2\";
    Real x(start=1) \"= -2\";
  initial equation
    a = x+1;
    x = 2*a;
    b = 2*a;
  equation
    der(x) = 1;
    der(a) = sin(time);
    der(b) = cos(time);
  end bug_3014;
end initializationTests;
"); getErrorString();

simulate(initializationTests.bug_3014, simflags="-lv=LOG_SOTI"); getErrorString();
val(a, 0.0); getErrorString();
val(b, 0.0); getErrorString();
val(x, 0.0); getErrorString();

