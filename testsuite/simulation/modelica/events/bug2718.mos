// name: bug2718
// keywords: event, not
// status: correct
// cflags:
// teardown_command: rm -rf eventTests.bug2718*  output.log
//
//

loadString("
package eventTests
model bug2718
  Real x,y,z(start=-0.002);
  Boolean b, b1;
algorithm
  b := not z > pre(x);
  when b then
    x := z+0.001;
  end when;
equation
  b1 = not z > pre(y);
  when b1 then
    y = z-0.001;
  end when;
  der(z) = 0.1*sin(50*time);
end bug2718;
end eventTests;
"); getErrorString();

simulate(eventTests.bug2718, stopTime=0.3); getErrorString();
val(x, 0.0);
val(x, 0.1);
val(x, 0.2);
val(x, 0.3);
val(y, 0.0);
val(y, 0.1);
val(y, 0.2);
val(y, 0.3);

