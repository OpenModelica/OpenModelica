loadFile("FuncDer.mo");
simulate(FuncDer,numberOfIntervals=2, tolerance=1e-5); // 2 intervals == 3 values
echo(false); // turns of output 

size:=readSimulationResultSize("FuncDer_res.mat");

res:=readSimulationResult("FuncDer_res.mat",{fn0,fn1,fn2},size); 

fn0:=res[1,1];
fn1:=res[2,1];
fn2:=res[3,1];

fn0:=1000*fn0;
fn1:=1000*fn1;
//fn2:=1000*fn2; // blir tydligen för stor värde eller nått för att köra floor på

fn0:=floor(fn0);

fn1:=floor(fn1);

fn2:=floor(fn2);

echo(true); // turns on output
fn0/1000.0;
fn1/1000.0;
fn2;
