// name: bug_2286
// keywords: boolean, array
// status: correct
//
//  tests for logical operations on boolen arrays
//

loadString("
 model bug_2286

    Boolean a[1] = {time > 0.4};
    Boolean b[1] = {time > 0.5};
    Boolean c[1] = {time > 0.6};
    Boolean d[1];
    Boolean e[1];
    Boolean f[1];
    Boolean g[1];

algorithm

    d:=a and b and c; 
    e:=not (a and b);
    f:=not a and b;
    g:=a or not b;

end bug_2286;
"); getErrorString();

simulate(bug_2286); getErrorString();

