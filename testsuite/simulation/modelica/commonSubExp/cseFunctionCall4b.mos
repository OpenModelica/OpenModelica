// name: cseFunctionCall4b
// keywords: cse
// status: correct
// cflags: -d=-newInst

loadString("
package CSE
  record R
    Real r1[3];
  end R;

  function f3
    input Real x1;
    input Real x2;
    output R y;
  algorithm
    y.r1[1] := 1.0;
    y.r1[2] := 2.0;
    y.r1[3] := 3.0;
  end f3;

  model FunctionCallTest4
    R y;
  equation
    y = f3(time, sin(time));
  end FunctionCallTest4;

end CSE;
"); getErrorString();

setDebugFlags("dumpCSE"); getErrorString();
setCommandLineOptions("--postOptModules+=wrapFunctionCalls"); getErrorString();
simulate(CSE.FunctionCallTest4); getErrorString();

