loadModel(Modelica, {"3.2.1"}); getErrorString();
loadModel(ThermoSysPro); getErrorString();

simulate(ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1, stopTime=1000);  getErrorString();

system("xz --keep --decompress --force ./ReferenceFiles/ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1.mat.xz");
(success, failVars) := OpenModelica.Scripting.diffSimulationResults(
"ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1_res.mat",
"ReferenceFiles/ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1.mat",
"ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1_diff.csv",
vars={"centrifugalPump.w"}); getErrorString();

if success then
  print("Files equal!\n");
else
  print("Files not equal!\n");
  for v in failVars loop
    curFile := "ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1." + v + ".html";
    success := writeFile(curFile, OpenModelica.Scripting.diffSimulationResultsHtml(v, "ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1_res.mat", "./ReferenceFiles/ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1.mat", 0.01)); getErrorString();
    if success then
      print("Created diff file " + curFile + "\n");
    else
      print("Creating diff file " + curFile + " failed\n");
    end if;
  end for; getErrorString();
end if;
system("rm -f ./ReferenceFiles/ThermoSysPro.Examples.SimpleExamples.TestCentrifugalPump1.mat");

