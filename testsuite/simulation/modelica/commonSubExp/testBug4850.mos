// name: cseSum
// keywords: cse
// status: correct
// cflags: -d=-newInst

loadString("
function foo
  input Real[:] x;
  output Real[:] y = x;
end foo;

model foobar
  Real y = sum(foo({time, time*2}));
end foobar;
");
getErrorString();

simulate(foobar);
getErrorString();

val(y, {0,1});

