loadString("
model functionTest9 
  function hv 
    input Real Tc; 
    input Real T; 
    output Real Ent2; 
  protected 
    Real temp, Ent;
  algorithm 
    temp := 298.15; 
    Ent :=0.0;
    if T> 298.15 then
      Ent := Ent + 1;
      Ent2 := Ent;
    elseif temp < 200 then
      Ent := Ent - 1;
      Ent2 := -Ent;
    else
      Ent := Ent + 1;
      Ent2 := Ent;
    end if; 
  end hv; 

parameter Real Tc_model = 33, T_model = 400; 
Real H; 

equation 
H = hv(Tc_model, T_model); 
end functionTest9;
"); getErrorString();

setCommandLineOptions("-d=evalFuncDump"); getErrorString();
simulate(functionTest9); getErrorString();

val(H, 0.0);

