// name: simpleForLoop
// keywords: NewBackend
// status: correct


loadString("
model simpleForLoop
  parameter Integer N=100;
  Real[N] x (each start=1, each fixed = true);
equation
  for i in 1:N loop
    der(x[i]) = 1/i * x[i];
   end for;
end simpleForLoop;
"); getErrorString();

setCommandLineOptions("--newBackend"); getErrorString();

simulate(simpleForLoop); getErrorString();

