// name: helloWorld
// keywords: NewBackend
// status: correct


loadString("
model helloWorld
  parameter Real a = 0.5;
  Real x (start=1, fixed = true);
equation
  der(x) = a * x;
end helloWorld;
"); getErrorString();

setCommandLineOptions("--newBackend"); getErrorString();

simulate(helloWorld); getErrorString();

val(a, 0.0);

