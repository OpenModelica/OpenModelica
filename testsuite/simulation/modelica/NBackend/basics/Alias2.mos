// name: Alias2
// keywords: NewBackend
// status: correct

loadString("
  model Alias
    Real y;             // not simple
    Real x, z;          // regular simple
    Real a, b, c;       // constant simple
  equation
    a = 2.0*c;
    x = z;
    c = time;
    y = time + c;
    der(x) = cos(time);
    der(z) = sin(time) + b;
  end Alias;
"); getErrorString();

setCommandLineOptions("--newBackend -d=dumprepl");
simulate(Alias); getErrorString();

