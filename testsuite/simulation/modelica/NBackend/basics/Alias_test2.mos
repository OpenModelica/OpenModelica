// name: Alias_test2
// keywords: NewBackend, Alias
// status: correct

loadString("
  model Test2
    // Throws an error if there are non literal nominal values.
    parameter Real p(fixed=false);
    Real y;             // not simple
    Real x(min=-200, max=200, start=-5, fixed=false, nominal=2);
    Real z(min=-100, max=100, start=-10, fixed=true, nominal=p);        // regular simple
  equation
    x = -2*z;
    y = time + 3.0;
    z = sin(time);
  end Test2;
"); getErrorString();
setCommandLineOptions("--newBackend -d=dumprepl");
simulate(Test2); getErrorString();

