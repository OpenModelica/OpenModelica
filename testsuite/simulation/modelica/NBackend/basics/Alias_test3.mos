// name: Alias_test3
// keywords: NewBackend, Alias
// status: correct

loadString("
  model Test3
    // Throws an error if more than one variable has StateSelect = ALWAYS.
    Real y;             // not simple
    Real x(min=-200, max=200, start=20, fixed=true, nominal=2, stateSelect=StateSelect.always);
    Real z(min=-100, max=100, start=-10, fixed=true, stateSelect=StateSelect.always);        // regular simple
  equation
    x = -2*z;
    y = time + 3.0;
    z = sin(time);
  end Test3;
"); getErrorString();
setCommandLineOptions("--newBackend -d=dumprepl");
simulate(Test3); getErrorString();

