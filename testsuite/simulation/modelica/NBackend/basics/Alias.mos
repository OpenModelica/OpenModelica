// name: Alias
// keywords: NewBackend
// status: correct

loadString("
  model Alias
    Real y;             // not simple
    Real x, z;          // regular simple
    Real a, b, c;       // constant simple
  equation
    a = b;
    b = c;
    x = z;
    c = 3.0;
    y = time + c;
    z = sin(time);
  end Alias;
"); getErrorString();

setCommandLineOptions("--newBackend -d=dumprepl");
simulate(Alias); getErrorString();

