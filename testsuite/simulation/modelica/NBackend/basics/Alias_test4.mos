// name: Alias_test4
// keywords: NewBackend, Alias
// status: correct

loadString("
  model Test4
    // Throws a warning if more than one variable is fixed and have the same start values.
    Real y;             // not simple
    Real x(min=-200, max=200, start=20, fixed=true, nominal=2);
    Real z(min=-100, max=100, start=-10, fixed=true);        // regular simple
  equation
    x = -2*z;
    y = time + 3.0;
    z = sin(time);
  end Test4;
"); getErrorString();
setCommandLineOptions("--newBackend -d=dumprepl");
simulate(Test4); getErrorString();

