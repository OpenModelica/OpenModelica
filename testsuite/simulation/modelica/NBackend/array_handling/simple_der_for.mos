// name: simple_der_for
// keywords: NewBackend
// status: correct

loadString("
model simple_der_for
  parameter Integer p = 10;
  Real[p+1] x;
equation
  for i in 1:p loop
    der(x[i]) = der(x[i+1]) + x[i] + sin(i*time);
  end for;
  der(x[3]) = cos(time);
end simple_der_for;

"); getErrorString();

setCommandLineOptions("--newBackend --generateSymbolicJacobian -d=symjacdump");

simulate(simple_der_for); getErrorString();

