// name: parameterSystem
// keywords: NewBackend
// status: correct

loadString("
model PS
  parameter Real a = 2;
  parameter Real b = a + 3;
  parameter Real c = d + 7;
  parameter Real d(fixed=false), e(fixed=false);
initial equation
  d = b + 5;
  e = a + 100000000;
end PS;

"); getErrorString();

//setCommandLineOptions("--newBackend"); getErrorString();

simulate(PS); getErrorString();

val(a,0.0);
val(b,0.0);
val(c,0.0);
val(d,0.0);
val(e,0.0);
