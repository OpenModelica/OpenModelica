// name: basic_IR
// keywords: NewBackend
// status: correct

loadString("
model basic_IR
  Real x,y;
  Real a,b;
equation
  der(x) = a * time;
  der(y) = b * time;
  x = a + b;
  y = x^2;
end basic_IR;
"); getErrorString();

setCommandLineOptions("--newBackend"); getErrorString();

simulate(basic_IR); getErrorString();

val(x, 0.0);
val(x, 1.0);
val(y, 0.0);
val(y, 1.0);
