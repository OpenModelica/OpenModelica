// name:     splitPartition
// keywords: synchronous features, sample, splitPartition, sorting
// status: correct
// cflags:

// ticket: #14610
loadString("
model splitPartition_MWE
  Clock clk = Clock(0.1);
  Real a = sample(time, clk);     // (Clock ticks 0.1, 0.2, 0.3, ...)
  Real sub1 = subSample(a, 2);    // (Clock ticks 0.2, 0.4, 0.6, ...)
  Real b = superSample(sub1, 2);  // (Clock ticks 0.1, 0.2, 0.3, ...)
end splitPartition_MWE;
"); getErrorString();

setCommandLineOptions("--newBackend"); getErrorString();

simulate(splitPartition_MWE, simflags="-lv=LOG_SYNCHRONOUS"); getErrorString();
diffSimulationResults("splitPartition_MWE_res.mat", "ReferenceFiles/splitPartition_MWE_res.mat", "splitPartition_MWE_diff.csv", 1e-8);

