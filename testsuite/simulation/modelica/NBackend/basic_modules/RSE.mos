// name: RSE
// keywords: NewBackend
// status: correct

loadString("
  model RSE
    Real x, y, z;
    Real a, b, c;
  equation
    a = b;
    b = c;
    x = z;
    c = time;
    y = time + c;
    z = sin(time);
  end RSE;
"); getErrorString();

setCommandLineOptions("--newBackend -d=dumprepl");
simulate(RSE); getErrorString();

