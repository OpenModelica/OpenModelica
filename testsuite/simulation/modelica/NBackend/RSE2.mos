// name: RSE2
// keywords: NewBackend
// status: correct

loadString("
  model RSE
    Real y;             // not simple
    Real x, z;          // regular simple
    Real a, b, c;       // constant simple
  equation
    a = c;
    x = z;
    c = 3.0;
    y = time + c;
    der(x) = cos(time);
    der(z) = sin(time) + b;
  end RSE;
"); getErrorString();

setCommandLineOptions("--newBackend -d=dumprepl");
simulate(RSE); getErrorString();

