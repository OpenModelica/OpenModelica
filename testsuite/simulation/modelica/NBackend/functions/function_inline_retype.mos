// name: function_inline_retype
// keywords: NewBackend
// status: correct
// cflags: --newBackend

loadString("
model function_inline_retype
  Real[2] x;
  Real y;
equation
  x[1] = time;
  x[2] = time*2;
  y = Modelica.Electrical.Polyphase.Functions.quasiRMS(x);
end function_inline_retype;
"); getErrorString();

simulate(function_inline_retype); getErrorString();

