// name: init_if_exp
// keywords: NewBackend, initialization, adjacency
// status: correct
// cflags: --newBackend

loadString("
model init_if_exp
  Real u, y;
equation
  1 = if initial() then y + y^2 else u;
  y = u^3 + u;
end init_if_exp;
"); getErrorString();

setCommandLineOptions("-d=dumpSimCode"); getErrorString();
simulate(init_if_exp); getErrorString();

