loadString("
model constStatelinearSys
  Real x, y;
equation
  der(y) = der(x);
  x = time - y;
end constStatelinearSys;
");
getErrorString();
simulate(constStatelinearSys); 
getErrorString();


val(der(x),0);
val(der(x),1);
val(der(y),0);
val(der(y),1);
val(x,0);
val(x,1);
val(y,0);
val(y,1);


