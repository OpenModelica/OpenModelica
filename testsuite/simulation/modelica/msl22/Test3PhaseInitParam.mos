// name:     Test3PhaseInitParam
// keywords: initialization, overdetermined, additial equations.
// cflags:   +std=2.x -d=-newInst
// status:   correct
// teardown_command: rm -f Test3Phase.Test3Phase* output.log
//  Simulate models and read in data.
//

loadFile("InitTest3Phase.mo");

simulate(Test3Phase.Test3PhaseInitParam,stopTime=10, tolerance=1e-5, numberOfIntervals=1000, simflags="-emit_protected");

"i_abc[:] at 0.0";
val(i_abc[1],0.0);
val(i_abc[2],0.0);
val(i_abc[3],0.0);

"i_dq0[:] at 0.0";
val(i_dq0[1],0.0);
val(i_dq0[2],0.0);
val(i_dq0[3],0.0);

"i_abc[:] at 10.0";
val(i_abc[1],10.0);
val(i_abc[2],10.0);
val(i_abc[3],10.0);

"i_dq0[:] at 10.0:";
val(i_dq0[1],10.0);
val(i_dq0[2],10.0);
val(i_dq0[3],10.0);

"u_abc[:] at 0.0";
val(u_abc[1],0.0);
val(u_abc[2],0.0);
val(u_abc[3],0.0);

"u_dq0[:] at 0.0";
val(u_dq0[1],0.0);
val(u_dq0[2],0.0);
val(u_dq0[3],0.0);

"u_abc[:] at 10.0";
val(u_abc[1],10.0);
val(u_abc[2],10.0);
val(u_abc[3],10.0);

"u_dq0[:] at 10.0:";
val(u_dq0[1],10.0);
val(u_dq0[2],10.0);
val(u_dq0[3],10.0);

"power :";
val(power,0);
val(power,10);


