// name: qss_example1
// status: correct
// cflags: -d=-newInst

loadString("
within ;
package qssTests
  model example1
    Real x(fixed=true, start=1);
  equation
    der(x) = -x;
  end example1;
end qssTests;
"); getErrorString();

simulate(qssTests.example1, startTime=0.0, stopTime=10.0, method="qss"); getErrorString();
val(x, {0.0, 2.5, 5.0, 10.0}); getErrorString();

