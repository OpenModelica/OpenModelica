// name:      totalTearing2
// keywords:  total tearing
// status:    correct
// cflags: -d=-newInst

loadString("
model totalTearing2
   Real x,y,z;
   parameter Real a=10, b=5, c=2;
equation
   a*x = b*z;
   a*x + b* sin(z) = y - c;
   c*y + time = x;
end totalTearing2;
"); getErrorString();

setCommandLineOptions("+d=backenddaeinfo,totaltearingdump, --totalTearing=2"); getErrorString();
simulate(totalTearing2); getErrorString();



