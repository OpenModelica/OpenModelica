loadModel(Modelica);
loadString("
model A
  parameter Modelica.Mechanics.MultiBody.Frames.Orientation R_start =
    Modelica.Mechanics.MultiBody.Frames.axesRotations(
      {1, 2, 3},
      angles_start,
      {0.0, 0.0, 0.0});

  parameter Real angles_start[3](each fixed = false);
initial equation
  angles_start = {0, 0, 0};
end A;

model Test5114
  A a[1];
end Test5114;
");
getErrorString();

setDebugFlags("newInst");
simulate(Test5114);
getErrorString();


