model NonFixedString
  parameter String s(fixed=false);
initial equation
  s = \"abc\";
end NonFixedString;
model FixedString
  parameter String s(fixed=true)=\"abc\";
end FixedString;
");getErrorString();
simulate(FixedString, simflags="-output=s");getErrorString();
simulate(NonFixedString, simflags="-output=s");getErrorString();
