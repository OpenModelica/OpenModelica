// name: testDAE10
// status: correct
// teardown_command: rm -f testDAE.p9* output.log
// cflags: -d=-newInst
// tests case for no states in the models

loadString("
model Test10
  Real x,y; 
equation
  -time = 10000*x*y-1;
  time = exp(-x) + exp(-y) - 1.0001;
end Test10;

");
getErrorString();

setCommandLineOptions("--daeMode");
simulate(Test10);
getErrorString();


val(x,0.0);
val(x,1.0);


