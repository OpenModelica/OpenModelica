// name:     terminal
// keywords: simulation, terminal, initial
// status: correct
// teardown_command: rm -rf testTerminal* output.log
// cflags: -d=-newInst
//
//  Simulate model that contain terminal and initial
//

loadString("
model testTerminal
  Real a(start=0.1);
algorithm
 if terminal() then
    a := 1.0;
 end if;
 if initial() then
   a := 0.5;
 end if;
end testTerminal;
"); getErrorString();

simulate(testTerminal); getErrorString();
val(a, 0.0);
val(a, 0.9);
val(a, 1.0);

