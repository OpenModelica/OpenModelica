// name: TestSolve
// status: correct
// cflags: -d=-newInst
//
// Checks that the backend knows how to solve certain expressions
//

loadString("
model ModelTestSolve8
  Real x(start=0);
  Real y(start=4);
  Real z(start=10);
  Real w;
equation
  tan(x) = time;
  tan(y) = time;
  tan(z) = time;
  w = atan(time);
end ModelTestSolve8;
"); getErrorString();

simulate(ModelTestSolve8, simflags="-lv LOG_NLS_V"); getErrorString();
val(x,0.5);
val(y,0.5);
val(z,0.5);
val(w,0.5);

