// name: TestSolve
// status: correct
// cflags: -d=-newInst
//
// Checks that the backend knows how to solve certain expressions
//

loadString("
model ModelTestSolve13
  Real x(start=-1);
  Real y;
  Real z(start=6);
  Real v(start=-6);
  Real w;
equation
  z = sin(time)^2;
  y = cos(z);
  cos(x+z+w) = time;
  cos(x+v+w) = time;
  w = acos(time);
end ModelTestSolve13;
"); getErrorString();

simulate(ModelTestSolve13, simflags="-lv LOG_NLS_V"); getErrorString();
val(x,0.5);
val(y,0.5);
val(z,0.5);
val(w,0.5);
val(v,0.5);

