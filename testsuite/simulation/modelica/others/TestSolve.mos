// name: TestSolve
// status: correct
// cflags: -d=-newInst
//
// Checks that the backend knows how to solve certain expressions
//

loadString("
model ModelTestSolve
  Real x,y,z;
equation
  ( 1 / x ) * 2 = 1;
  ( y / 1 ) * 2 = 1;
  4 = ( 1 / z ) * 3;
end ModelTestSolve;
"); getErrorString();

simulate(ModelTestSolve, simflags="-lv LOG_NLS_V"); getErrorString();
val(x,0.5);
val(y,0.5);
val(z,0.5);

