// name: filterTest
// keywords: mat, variable filter
// status: correct
// cflags:
// teardown_command: rm -rf otherTests.filterTest_* output.log
// cflags: -d=-newInst
//
//  #2088
//


loadString("
within ;
package otherTests
  model filterTest
    parameter Real Var1 = 1;
    Real Var2X(start=0, fixed=true);
    Real Var3;
  equation
    Var3 = Var2X;
    der(Var2X) = Var1;
  end filterTest;
end otherTests;
"); getErrorString();

simulate(otherTests.filterTest, variableFilter=".*[^X]"); getErrorString();
val(Var1, 0.0);
val(Var3, 0.0);

