// name: TestAsub
// status: correct
// teardown_command: rm -f TestAsub_* TestAsub TestAsub.exe
// cflags: -d=-newInst

loadString("
model TestAsub
  function func
    input Integer idx;
    output Integer value;
  protected
    constant Integer[4] values = {1, 2, 3, 4};
  algorithm
    value := values[idx];
  end func;

  parameter Integer idx = 1;
  parameter Integer p1 = func(1);
  parameter Integer p2 = func(idx);
end TestAsub;
");
simulate(TestAsub);
val(p1,1234);
val(p2,5678);

