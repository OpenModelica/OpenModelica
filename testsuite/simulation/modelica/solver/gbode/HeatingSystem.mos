// name: HeatingSystem
// status: correct
// teardown_command: rm -rf temp_HeatingSystem/
//
// Test all available generic Runge-Kutta solvers with all available non-linear solver methods.

loadFile("HeatingSystemDiscrete.mo");
getErrorString();

loadString("
model HeatingSystem
  extends HeatingSystemDiscrete.HeatingSystem;
end HeatingSystem;
"); getErrorString();

echo(false);
system("rm -rf temp_HeatingSystem/ && mkdir temp_HeatingSystem/");
cd("temp_HeatingSystem");
echo(true);

setCommandLineOptions("--generateSymbolicJacobian"); getErrorString();

// Build model
buildModel(HeatingSystem, stopTime=864000); getErrorString();

// Create reference results
system("./HeatingSystem -s=dassl -r HeatingSystem_ref.mat -override=\"tolerance=1e-8,stopTime=864000\"", "refSimulation.log");
print(readFile("refSimulation.log"));

// Simulate with gauss2 & KINSOL+KLU
system("./HeatingSystem -s=gbode -gbm=gauss5 -gberr=1 -gbnls=kinsol -override=\"tolerance=1e-6,stopTime=864000\"", "HeatingSystem_gauss5.log");
print(readFile("HeatingSystem_gauss5.log"));

diffSimulationResults(actualFile = "HeatingSystem_res.mat",
                      expectedFile = "HeatingSystem_ref.mat",
                      diffPrefix = "diff",
                      vars = {"Td"});
getErrorString();

// Simulate with sdirk2 & KINSOL+KLU
system("./HeatingSystem -s=gbode -gbm=fehlberg78 -gbratio=1 -override=\"tolerance=1e-6,stopTime=864000\"", "HeatingSystem_fehlberg78_1.log");
print(readFile("HeatingSystem_fehlberg78_1.log"));

diffSimulationResults(actualFile = "HeatingSystem_res.mat",
                      expectedFile = "HeatingSystem_ref.mat",
                      diffPrefix = "diff",
                      vars = {"Td"});
getErrorString();

// Simulate with sdirk2 & KINSOL+KLU
system("./HeatingSystem -s=gbode -gbm=fehlberg78 -override=\"tolerance=1e-6,stopTime=864000\"", "HeatingSystem_fehlberg78.log");
print(readFile("HeatingSystem_fehlberg78.log"));

diffSimulationResults(actualFile = "HeatingSystem_res.mat",
                      expectedFile = "HeatingSystem_ref.mat",
                      diffPrefix = "diff",
                      vars = {"Td"});
getErrorString();

