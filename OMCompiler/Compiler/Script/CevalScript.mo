    case (cache,env,"generateSeparateCode",{v,Values.BOOL(b)},_)
      equation
        p = SymbolTable.getAbsyn();
        sp = SymbolTable.getSCode();
        name = getTypeNameIdent(v);
        setGlobalRoot(Global.instOnlyForcedFunctions,SOME(true));
